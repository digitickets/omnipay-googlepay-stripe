{
    "name": "digitickets/omnipay.googlepay.stripe",
    "type": "library",
    "description": "Custom OmniPay driver for Google Pay (with Stripe)",
    "keywords": ["omnipay", "googlepay", "stripe"],
    "homepage": "https://bitbucket.org/digitickets/omnipay-google-pay-stripe",
    "autoload": {
        "psr-4": { "Digitickets\\OmnipayGooglePayWithStripe\\" : "src/" }
    },
    "require": {
        "omnipay/common": "3.1.2",
        "guzzlehttp/psr7": "2.1.0",
        "stripe/stripe-php": "^7.116"
    },
    "require-dev": {
        "omnipay/tests": "~2.0",
        "squizlabs/php_codesniffer": "~1.5"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "master"
        }
    },
    "prefer-stable": true
}
